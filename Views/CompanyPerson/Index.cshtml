<!-- CONTAINER CARD START -->
<div class="col-sm-12">
  <div class="card shadow-custom border border-custom">
    <!-- CARD HEADER START -->
    <div
      class="card-header shadow grid bg-gray-200 bg-gradient border-bottom d-md-flex align-items-center justify-content-between gap-2 px-3 py-2">
      <div class="d-flex gap-2 mb-2 mb-md-0">
        <div class="input-group input-group-outline input-group-sm bg-white rounded">
          <label class="form-label">Arama Yap</label>
          <input type="text" id="filter-text-boxCompanyPersons" class="form-control form-control-sm text-sm"
            oninput="onFilterTextBoxChanged(gridOptionsCompanyPersonsApi,this)">
        </div>
        <div class="btn-group d-flex">
          <button type="button" class="btn btn-icon bg-gradient bg-white border border-custom text-dark px-2 py-1 mb-0"
            onclick=" customSizeToFit(gridOptionsCompanyPersonsApi)" data-bs-tooltip="tooltip"
            data-bs-placement="bottom" title="Sığdır" data-container="body" data-animation="true">
            <span class="btn-inner--icon">
              <i class="material-icons text-dark fs-6">
                horizontal_distribute
              </i>
            </span>
          </button>
          <button type="button" class="btn btn-icon bg-gradient bg-white border border-custom text-dark px-2 py-1 mb-0"
            onclick="customAutoSizeAll(gridOptionsCompanyPersonsApi,false)" data-bs-tooltip="tooltip"
            data-bs-placement="bottom" title="Otomatik Boyutlandır" data-container="body" data-animation="true">
            <span class="btn-inner--icon">
              <i class="material-icons text-dark fs-6">
                vertical_distribute
              </i>
            </span>
          </button>
          <button type="button" class="btn btn-icon bg-gradient bg-white border border-custom text-dark px-2 py-1 mb-0"
            onclick="customAutoSizeAll(gridOptionsCompanyPersonsApi,true)" data-bs-tooltip="tooltip"
            data-bs-placement="bottom" title="Otomatik Boyutlandır (Başlıkları Önemseme)" data-container="body"
            data-animation="true">
            <span class="btn-inner--icon">
              <i class="material-icons text-dark fs-6">
                density_large
              </i>
            </span>
          </button>
          <button type="button" class="btn btn-icon bg-gradient bg-white border border-custom text-dark px-2 py-1 mb-0"
            onclick="gridExportExcel(gridOptionsCompanyPersonsApi)" data-bs-tooltip="tooltip" data-bs-placement="bottom"
            title="Excel'e Aktar" data-container="body" data-animation="true">
            <span class="btn-inner--icon">
              <i class="material-icons text-dark fs-6">
                upload_file
              </i>
            </span>
          </button>
          <button type="button" class="btn btn-icon bg-gradient bg-white border border-custom text-dark px-2 py-1 mb-0"
            onclick="gridLayoutSave(gridOptionsCompanyPersonsApi,'GridCompanyPersons',getCookie('testUserId'));"
            data-bs-tooltip="tooltip" data-bs-placement="bottom" title="Düzeni Kaydet" data-container="body"
            data-animation="true">
            <span class="btn-inner--icon">
              <i class="material-icons text-dark fs-6">
                save
              </i>
            </span>
          </button>
        </div>
      </div>
      <div class="btn-group d-flex">
        <button class="btn btn-icon bg-white border border-custom px-2 py-1 mb-0" data-bs-toggle="modal"
          data-bs-target="#modalCompanyPersons" data-bs-tooltip="tooltip" data-bs-placement="bottom" title="Ekle"
          data-container="body" data-animation="true">
          <span class="btn-inner--icon">
            <i class="material-icons text-dark fw-bold fs-6">
              add
            </i>
          </span>
        </button>
        <button class="btn btn-icon bg-white border border-custom px-2 py-1 mb-0"
          onclick="agGridSelectedrow(gridOptionsCompanyPersonsApi, 'CompanyPersons', 'Güncelle', 'Operatör Listesi');"
          data-bs-tooltip="tooltip" data-bs-placement="bottom" title="Düzenle" data-container="body"
          data-animation="true">
          <span class="btn-inner--icon">
            <i class="material-icons text-dark fs-6">
              edit
            </i>
          </span>
        </button>
        <button class="btn btn-icon bg-white border border-custom px-2 py-1 mb-0"
          onclick="agGridSelectedrow(gridOptionsCompanyPersonsApi, 'CompanyPersons', 'Sil', 'Operatör Listesi');"
          data-bs-tooltip="tooltip" data-bs-placement="bottom" title="Sil" data-container="body" data-animation="true">
          <span class="btn-inner--icon">
            <i class="material-icons text-dark fs-6">
              delete
            </i>
          </span>
        </button>
      </div>
    </div>
    <!-- CARD HEADER END -->
    <!-- CARD BODY START -->
    <div class="card-body p-3 grid-container">
      <div id="GridCompanyPersons" class="ag-theme-alpine">
      </div>
    </div>
    <!-- CARD BODY END -->
  </div>
</div>
<!-- CONTAINER CARD END -->

<!-- MODAL START -->
<div class="modal fade" id="modalCompanyPersons" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
  style="z-index: 9999;" aria-hidden="true">
  <div class="modal-dialog modal-dialog-top" role="document">
    <div class="modal-content">
      <!-- MODAL HEADER START  -->
      <div class="modal-header bg-gradient bg-gray-200 border-bottom shadow py-2">
        <h6 class="modal-title" id="exampleModalLabel">Operatör Listesi Bilgileri</h6>
        <button type="button" class="btn-close text-dark d-flex align-items-center justify-content-center"
          data-bs-dismiss="modal" aria-label="Close">
          <span class="fs-4" aria-hidden="true">×</span>
        </button>
      </div>
      <!-- MODAL HEADER END -->
      <!-- MODAL BODY START -->
      <div class="modal-body">
        <div class="row">
          <form id="formCompanyPersons">
            <div class="col-sm-12">
              <div class="row">
                <div class="col-12 mb-2 d-none">
                  <div class="input-group input-group-static">
                    <label class="fw-bolder text-sm mb-1" id="lbl_id" for="id">id</label>
                    <input type="text" class="form-control form-control-sm form-control-custom" placeholder="id"
                      id="id">
                  </div>
                </div>
                <div class="col-12 mb-2 d-none">
                  <div class="input-group input-group-static">
                    <label class="fw-bolder text-sm mb-1" id="lbl_createdAt" for="createdAt">createdAt</label>
                    <input type="text" class="form-control form-control-sm form-control-custom" placeholder="createdAt"
                      id="createdAt">
                  </div>
                </div>
                <div class="col-12 mb-2 d-none">
                  <div class="input-group input-group-static">
                    <label class="fw-bolder text-sm mb-1" id="lbl_updatedAt" for="updatedAt">updatedAt</label>
                    <input type="text" class="form-control form-control-sm form-control-custom" placeholder="updatedAt"
                      id="updatedAt">
                  </div>
                </div>
                <div class="col-12 mb-2 d-none">
                  <div class="input-group input-group-static">
                    <label class="fw-bolder text-sm mb-1" id="lbl_active" for="active">active</label>
                    <input type="text" class="form-control form-control-sm form-control-custom" placeholder="active"
                      id="active">
                  </div>
                </div>
                <div class="col-12 mb-2">
                  <div class="dropdown-custom">
                    <label id="lbl_companyID" for="companyID" class="fw-bolder mb-1 ms-0">Şirket</label>
                    <select name="companyID" id="companyID">
                      <option value="">Lütfen bir seçim yapınız...</option>
                    </select>
                  </div>
                </div>
                <div class="col-12 mb-2">
                  <div class="input-group input-group-static">
                    <label class="fw-bolder text-sm mb-1" id="lbl_personalNo" for="personalNo">Personel No</label>
                    <input type="text" class="form-control form-control-sm form-control-custom"
                      placeholder="Personel No" id="personalNo">
                  </div>
                </div>
                <div class="col-12 mb-2">
                  <div class="input-group input-group-static">
                    <label class="fw-bolder text-sm mb-1" id="lbl_personalName" for="personalName">Personel Adı</label>
                    <input type="text" class="form-control form-control-sm form-control-custom"
                      placeholder="Personel Adı" id="personalName">
                  </div>
                </div>
              </div>
            </div>
          </form>
        </div>
      </div>
      <!-- MODAL BODY END  -->
      <!-- MODAL FOOTER START  -->
      <div class="modal-footer">
        <button type="button" id="closeCompanyPersons"
          class="btn btn-sm btn-outline-secondary shadow border border-custom m-0 me-2"
          data-bs-dismiss="modal">Kapat</button>
        <button type="button" id="submitCompanyPersons" class="btn btn-sm bg-gradient btn-brand-color shadow m-0 me-1"
          onclick="ModalInsertAndUpdate('modalCompanyPersons');">Kaydet</button>
      </div>
      <!-- MODAL FOOTER END  -->
    </div>
  </div>
</div>
<!-- MODAL END  -->

<!-- MODAL SCRIPT  -->
<script>
  $(document).ready(function () {
    initializeGrid("CompanyPersons",);
  });

  ModalOpenMethods("CompanyPersons", "update", "insert", "defaultFunction2");

  async function update(data) {
    $('#companyID').val(data.companyID ?? data.CompanyID);
  }
  async function insert() {
  }

  async function defaultFunction2() {
   // await dropdownFill('vw_Companies', 'formCompanyPersons', 'companyID', 'Name'); 
    await GetMethodDropdownFill(null,"Companies","GetAll",null,"formCompanyPersons","companyID","Name");

  }
</script>
