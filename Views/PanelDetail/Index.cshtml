<!-- CARD BODY START -->
<div class="col-sm-12 mb-2" id="card-detail">
    <div class="row">
        <!-- CARD 1 START -->
        <div class="col-sm-6 mb-2">
            <div class="card shadow-custom border border-custom h-100">
                <div class="card-header py-2 grid border-bottom bg-gray-200 bg-gradient shadow">
                    <h6 class="mb-0 text-sm lh-base">Firma Bilgileri</h6>
                </div>
                <div class="card-body py-3 m-0 text-sm text-dark">
                    <div class="mb-1">
                        <label class="text-dark ms-0 fw-bold">Şirket:</label> <span id="lblCompany_Name"></span>
                    </div>
                    <div class="mb-1">
                        <label class="text-dark ms-0 fw-bold">Şube:</label> <span id="lblBranch_Name"></span>
                    </div>
                    <div class="mb-1">
                        <label class="text-dark ms-0 fw-bold">Bölüm:</label> <span id="lblDepartment_Name"></span>
                    </div>
                    <div class="mb-1">
                        <label class="text-dark ms-0 fw-bold">Grup:</label> <span id="lblGroup_Name"></span>
                    </div>
                </div>
            </div>
        </div>
        <!-- CARD 1 END -->

        <!-- CARD 2 START -->
        <div class="col-sm-6 mb-2">
            <div class="card shadow-custom border border-custom h-100">
